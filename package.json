{
  "name": "monorepo",
  "private": true,
  "author": "Innei",
  "license": "MIT",
  "scripts": {
    "prepare": "pnpm run build",
    "build:ui": "pnpm run build -C packages/ui",
    "build:utils": "pnpm run build -C packages/utils",
    "build": "pnpm run build -r",
    "dev:web": "concurrently --kill-others \"pnpm run dev -C packages/web\" \"pnpm run watch -C packages/ui\" \"pnpm run watch -C packages/utils\""
  },
  "devDependencies": {
    "@innei-util/eslint-config-react-ts": "0.2.0",
    "@innei-util/eslint-config-ts": "0.4.0",
    "@innei-util/prettier": "0.2.2",
    "@types/lodash-es": "4.17.6",
    "@types/node": "17.0.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "lodash-es": "4.17.21",
    "typescript": "4.5.5",
    "vite": "2.8.2",
    "vite-tsconfig-paths": "^3.4.0",
    "concurrently": "7.0.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}